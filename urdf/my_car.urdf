<?xml version="1.0" encoding="UTF-8"?>
<robot name="mycar">
    <link name="base_footprint">
        <visual>
            <geometry>
                <sphere radius="0.001" />
            </geometry>
        </visual>
    </link>

    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.5 0.4 0.2" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <material name="blue">
                <color rgba="0 0 1 0.5" />
            </material>
        </visual>
    </link>

    <joint name="base_link2base_footprint" type="fixed" >
        <parent link="base_footprint" />
        <child link="base_link" />
        <origin xyz="0 0 0.1" />
    </joint>

    <link name="left_wheel">
        <visual>
            <geometry>
                <cylinder radius= "0.1" length="0.02" />
            </geometry>
            <material name="green" >
                <color rgba="0 1 0 1" />
            </material>
            <origin xyz="0 0 0" rpy="1.57 0 0 " />
        </visual>
    </link>
    <link name="right_wheel">
        <visual>
            <geometry>
                <cylinder radius= "0.1" length="0.02" />
            </geometry>
            <material name="green" >
                <color rgba="0 1 0 1" />
            </material>
            <origin xyz="0 0 0" rpy="-1.57 0 0 " />
        </visual>
    </link>

    <joint name="left_wheel_base" type="continuous">
        <parent link="base_link" />
        <child link="left_wheel" />
        <origin xyz="0 0.21 -0.1" rpy="0 0 0" />
        <axis xyz="0 1 0" />
    </joint>
    <joint name="right_wheel_base" type="continuous">
        <parent link="base_link" />
        <child link="right_wheel" />
        <origin xyz="0 -0.21 -0.1" rpy="0 0 0" />
        <axis xyz="0 1 0" />
    </joint>

    <link name="front_wheel">
        <visual>
            <geometry>
                <sphere radius="0.05" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <material name="yellow">
                <color rgba="1 1 0 1" />
            </material>
        </visual>
    </link>

    <joint name="front_wheel_base" type="continuous" >
        <parent link="base_link" />
        <child link="front_wheel" />
        <origin xyz="0.2 0 -0.15" rpy="0 0 0" />
        <axis xyz="1 1 1" />
    </joint>

    <link name="back_wheel">
        <visual>
            <geometry>
                <sphere radius="0.05" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <material name="yellow">
                <color rgba="1 1 0 1" />
            </material>
        </visual>
    </link>

    <joint name="back_wheel_base" type="continuous" >
        <parent link="base_link" />
        <child link="back_wheel" />
        <origin xyz="-0.2 0 -0.15" rpy="0 0 0" />
        <axis xyz="1 1 1" />
    </joint>

    <link name="camera">
        <visual>
            <geometry>
                <box size="0.02 0.05 0.04" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <material name="red">
                <color rgba="1 0 0 0.5" />
            </material>
        </visual>
    </link>

    <joint name="camera2baselink" type="continuous">
        <parent link="base_link" />
        <child link="camera" />
        <origin xyz="0.2 0 0.12" rpy="0 0 0" />
        <axis xyz="0 0 1" />
    </joint>

</robot>
